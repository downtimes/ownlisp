{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "Ownlisp syntax",
    "scopeName": "source.olp",
    "patterns": [
        {
            "include": "#keywords"
        },
        {
            "include": "#strings"
        }
    ],
    "repository": {
        "keywords": {
            "patterns": [
                {
                    "name": "keyword.operator.ownlisp",
                    "match": "(\\+|-|/|min|max|\\^|%)"
                }
            ]
        },
        "strings": {
            "patterns": [
                {
                    "name": "string.quoted.double.ownlisp",
                    "begin": "\"",
                    "end": "\""
                }
            ]
        }
    }
}