{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "Ownlisp syntax",
    "scopeName": "source.olp",
    "patterns": [
        {
            "name": "comment.ownlisp",
            "match": ";.*"
        },
        {
            "name": "string.quoted.ownlisp",
            "begin": "\"",
            "end": "\""
        },
        {
            "name": "keyword.control.ownlisp",
            "match": "if|def|let|head|tail|eval|list|load|error|print|join"
        },
        {
            "name": "keyword.operator.ownlisp",
            "match": "\\+|\\*|-|/|%|min|max|\\^"
        },
        {
            "name": "constant.language.ownlisp",
            "match": "true|false"
        }
   ]
}
    