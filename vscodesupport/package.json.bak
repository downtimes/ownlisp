{
    "name": "ownlisp",
    "displayName": "Ownlisp",
    "version": "0.0.1",
    "description": "Adds support for the Ownlisp language",
    "publisher": "Michael Auracher",
    "engines": {
        "vscode": "^1.22.0"
    },
    "author": {
        "name": "Michael Auracher"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ownlisp",
                "configuration": "./language-configuration.json",
                "extensions": [
                    ".olp"
                ]
            }
        ],
        "grammars": [
            {
                "language": "ownlisp",
                "scopeName": "source.ownlisp",
                "path": "./syntaxes/ownlisp.tmLanguage.json"
            }
        ]
    }
}