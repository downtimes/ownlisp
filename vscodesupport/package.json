{
	"name": "ownlisp",
	"displayName": "ownlisp",
	"description": "pestfile Syntax Highlighting",
	"version": "0.0.1",
	"publisher": "xoronic",
	"repository": {
		"url": "https://github.com/Xoronic/pest-Syntax-Highlighting-for-vscode",
		"type": "git"
	},
	"engines": {
		"vscode": "^1.21.0"
	},
	"categories": [
		"Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "ownlisp",
				"aliases": [
					"pestfile",
					"pestfile"
				],
				"extensions": [
					".olp"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ownlisp",
				"scopeName": "source.ownlisp",
				"path": "./syntaxes/pestfile.tmLanguage.json"
			}
		]
	}
}
